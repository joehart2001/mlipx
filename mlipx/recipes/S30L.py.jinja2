{% if standalone %}
import mlipx
import zntrack
from models import MODELS

project = zntrack.Project()
{% endif %}


S30L_node_dict = {}
S30L_node_list = []

for model_name, model in MODELS.items():
    with project.group(model_name):
        S30L_benchmark = mlipx.S30LBenchmark(
            model =model,
            model_name=model_name,
        )
        S30L_node_dict[model_name] = S30L_benchmark
        S30L_node_list.append(S30L_benchmark)

    

{% if standalone %}
project.build()
{% endif %}