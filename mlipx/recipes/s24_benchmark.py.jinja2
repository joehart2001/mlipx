import mlipx
import zntrack
from models import MODELS

project = zntrack.Project()
{% endif %}


s24_dict = {}
s24_list = []
for model_name, model in MODELS.items():
    with project.group(model_name, "s24"):
        s24 = mlipx.S24Benchmark(
                model=model,
                model_name=model_name,
        )
        s24_dict[model_name] = s24
        s24_list.append(s24)
    

{% if standalone %}
project.build()
{% endif %}