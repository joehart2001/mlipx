{% if standalone %}
import mlipx
import zntrack
from models import MODELS

project = zntrack.Project()
{% endif %}


protein_ligand_dict = {}
protein_ligand_list = []

for model_name, model in MODELS.items():
    with project.group(model_name):
        protein_ligand_benchmark = mlipx.ProteinLigandBenchmark(
            model =model,
            model_name=model_name,
        )
        protein_ligand_dict[model_name] = protein_ligand_benchmark
        protein_ligand_list.append(protein_ligand_benchmark)

    

{% if standalone %}
project.build()
{% endif %}